sum1ton:
        mov     r2, #$f:0$
        mov     r1, #1
1:
        $fQ:add$ $fQ:r2$, r1
        add     $fQ:r1$, #1
        $fQ:cmp$ r1, r0
        $fQ:bne,blt$ 1b
        mov     $fQ:r0$, r2
        bx      lr
