{"quizspec":"dkruger.qspec", "name":"CPE390 Floating Point"}
{"qt":"pcode", "name":"Rules of Inf and NaN"}
1.0 / 0.0 = $inf$
-1.0 / 0.0 = $-inf$
0.0 / 0.0 = $nan$
sqrt(1.0 / 0.0) = $inf$
sqrt(-1.0) = $nan$
2.0 / 0. = $inf$
cos(1.0 / 0.0) = $inf$
sin(3.0 / 0.0 * 0.0) = $nan$

{"qt":"pcode", "name":"true or false?"}
1.0 / 0.0 == -1.0 / 0.0 ? $false$
2.0 / 0.0 == 1.0 / 0.0 + 1 ? $true$
0.0 / 0.0 == sqrt(-1.0) ? $false$
sqrt(-1.0) != sqrt(-1.0) ? $false$
